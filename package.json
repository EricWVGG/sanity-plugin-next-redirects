{
  "name": "sanity-plugin-next-redirects",
  "version": "0.2.1",
  "description": "Facilitates a dynamic redirect table in NextJS with Sanity.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EricWVGG/sanity-plugin-next-redirects.git"
  },
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "license": "MIT",
  "author": "Eric Jacobsen <eric@wvgg.co>",
  "sideEffects": false,
  "type": "commonjs",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "link-watch": "plugin-kit link-watch",
    "prepublishOnly": "npm run build",
    "watch": "pkg-utils watch --strict"
  },
  "dependencies": {
    "@sanity/icons": "^3.7.4",
    "@sanity/incompatible-plugin": "^1.0.5",
    "@sanity/ui": "^3.1.11",
    "pluralize-esm": "^9.0.5",
    "sanity-advanced-validators": "^0.6.0"
  },
  "devDependencies": {
    "@sanity/pkg-utils": "8.1.12",
    "@sanity/plugin-kit": "^4.0.20",
    "@types/react": "^19.2.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sanity": "^4.14.2",
    "styled-components": "^6.1.19",
    "typescript": "^5.9.3",
    "yalc": "1.0.0-pre.53"
  },
  "peerDependencies": {
    "next-sanity": "^11.6.6",
    "pluralize-esm": "^9.0.5",
    "react": "^19.2.0",
    "sanity": "^4.14.2"
  },
  "engines": {
    "node": ">=18"
  },
  "sanityPlugin": {
    "linkWatch": {
      "command": "npm run watch",
      "extensions": "js,png,svg,gif,jpeg,css"
    },
    "verifyPackage": {
      "eslintImports": false
    }
  }
}
